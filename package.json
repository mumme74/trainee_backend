{
  "name": "Trainee_backend",
  "version": "0.0.2",
  "description": "Backend to Trainee system. It's a system that trains students.",
  "main": "server/index.js",
  "scripts": {
    "test-old": "NODE_ENV=test mocha --timeout 5000 __tests__/server/**/*.test.js",
    "backend-test": "NODE_ENV=test && node_modules/jest/bin/jest.js $INIT_CWD/server",
    "report": "NODE_ENV=test nyc --reporter=lcov mocha --timeout 5000 __tests__/server/**/*.test.js",
    "text-report": "NODE_ENV=test nyc --reporter=text-sumary mocha --timout 5000",
    "nyc": "NODE_ENV=test nyc mocha --timeout 5000 __tests__/server/**/*.test.js",
    "build": "npm run backend-build && npm run frontend-build",
    "backend-build": "tsc --build tsconfig.server.json",
    "backend-start-dev": "NODE_ENV=development nodemon -e ts,js --watch server/ --exec \"npm run backend-build && node build/server/index.js --harmony\"",
    "backend-start": "NODE_ENV=production node build/server/index.js",
    "frontend-start-dev": "NODE_ENV=development npm run start --prefix client",
    "frontend-build": "tsc --build tsconfig.client.json",
    "frontend-translation": "NODE_ENV=development npm run translation-parse --prefix client",
    "backend-lint": "eslint 'server/**/*.{js,ts}' --quiet --fix",
    "frontend-lint": "eslint 'client/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "lint": "npm run frontend-lint && npm run backend-lint",
    "lint-all": "eslint 'client/**/*.{js,jsx,ts,tsx}' 'server/**/*.{js,jsx,ts,tsx}' --quiet --fix"
  },
  "author": "Fredrik Johansson (mumme74 @ github)",
  "license": "ISC",
  "dependencies": {
    "accepts": "^1.3.7",
    "bcrypt": "^5.0.1",
    "bson": "^5.4.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^16.3.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-promise-router": "^4.1.0",
    "express-rate-limit": "^6.7.1",
    "google-auth-library": "^8.9.0",
    "graphql": "^16.7.0",
    "graphql-http": "^1.21.0",
    "graphql-language-service": "^5.1.7",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "i18next": "^23.2.0",
    "i18next-http-backend": "^2.2.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-google-verify-token": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@jest-mock/express": "^2.0.0",
    "@types/accepts": "^1.3.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.4.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.4.0",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-jest": "^29.6.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.24.0",
    "faker": "^6.6.6",
    "i18next-parser": "^8.3.0",
    "jest": "^29.6.0",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rewire": "^6.0.0",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.4"
  }
}
